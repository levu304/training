{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../../tsconfig.options.json",
  "compilerOptions": {
    "noEmit": true,
    "downlevelIteration": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "types": ["react/experimental"],
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      ":/*": ["./src/*"]
    }
  }
}
